{
  "name": "suburbmates",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "env:check": "tsx scripts/check-env.ts",
    "dev": "npm run env:check && next dev -p 3000",
    "build": "NODE_ENV=production next build --webpack",
    "start": "next start",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "stripe": "stripe",
    "stripe:listen": "stripe listen --forward-to http://localhost:3000/api/stripe/webhook",
    "db:seed": "tsx docs/seed.ts",
    "genkit:dev": "genkit start --port 4001 -- npm run dev",
    "mcp:verify": "tsx src/ai/mcp/verify.ts",
    "firebase:emulators": "firebase emulators:start",
    "firebase:emulators:stop": "firebase emulators:exec 'echo done'",
    "dev:emulator": "npm run firebase:emulators & npm run dev",
    "emulator:test": "firebase emulators:exec --only firestore,auth 'node -e \"console.log(\\'Emulators running successfully!\\')\"'",
    "emulator:export": "firebase emulators:export ./emulator-data",
    "emulator:import": "firebase emulators:start --import=./emulator-data",
    "dev:start": "npm-run-all --parallel firebase:emulators dev --print-label --print-name",
    "dev:test": "npm-run-all --sequential env:check lint typecheck",
    "dev:ci": "npm-run-all --sequential env:check lint typecheck",
    "genkit:start": "genkit start --port 4002",
    "dev:full": "npm-run-all --parallel firebase:emulators dev genkit:start --print-label --print-name",
    "audit:test": "playwright test tests/design-system-full.spec.ts",
    "audit:update": "playwright test tests/design-system-full.spec.ts --update-snapshots",
    "clean": "bash -lc 'rm -rf .next .next-* .next-3000 .next-3001 .next-3002 playwright-report test-results coverage emulator-data firebase-exports .idx .mcp-env modified database-debug.log firestore-debug.log .genkit/runtimes functions/.genkit/runtimes functions/.genkit/servers functions/.genkit/traces functions/.genkit/traces_idx; find ./node_modules -type d -name coverage -prune -exec rm -rf {} + 2>/dev/null || true; find ./functions/node_modules -type d -name coverage -prune -exec rm -rf {} + 2>/dev/null || true'",
    "clean:ignored": "git clean -fdX",
    "clean:all": "npm run clean && npm run clean:ignored",
    "grant:admin": "tsx scripts/grant-admin.ts",
    "grant:admin:prod": "ADMIN_TARGET=prod tsx scripts/grant-admin.ts",
    "grant:admin:emulator": "ADMIN_TARGET=emulator tsx scripts/grant-admin.ts",
    "env:set:service": "tsx scripts/set-service-account-env.ts",
    "mcp:typescript": "tsx tools/mcp-typescript/src/index.ts",
    "mcp:typescript:build": "tsc -p tools/mcp-typescript/tsconfig.json",
    "mcp:typescript:inspector": "npm run mcp:typescript:build && npx @modelcontextprotocol/inspector tools/mcp-typescript/build/index.js"
  },
  "dependencies": {
    "@genkit-ai/google-genai": "^1.22.0",
    "@genkit-ai/mcp": "^1.22.0",
    "@genkit-ai/next": "^1.22.0",
    "@hookform/resolvers": "^5.2.2",
    "@modelcontextprotocol/server-filesystem": "^2025.8.21",
    "@modelcontextprotocol/server-memory": "^2025.9.25",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@vercel/analytics": "^1.3.1",
    "@vis.gl/react-google-maps": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "firebase-storage": "^1.0.3",
    "framer-motion": "^12.23.24",
    "genkit": "^1.22.0",
    "lucide-react": "^0.548.0",
    "mime-types": "^3.0.1",
    "next": "^16.0.1",
    "next-pwa": "^5.6.0",
    "patch-package": "^8.0.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.3.0",
    "resend": "^6.3.0",
    "stripe": "^19.2.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@playwright/test": "^1.56.1",
    "@types/google.maps": "^3.55.11",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "genkit-cli": "^1.20.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.2",
    "typescript": "^5",
    "typescript-eslint": "^8.46.2"
  }
}
